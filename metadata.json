{
  "name": "datadog-datadog_agent",
  "version": "0.0.1",
  "author": "Mike Laning (<mike.laning@datadoghq.com>)",
  "summary": "Install the Datadog monitoring agent and report Puppet runs to Datadog",
  "license": "Apache-2.0",
  "source": "git://github.com/DataDog/puppet-datadog-agent.git",
  "project_page": "https://github.com/DataDog/puppet-datadog-agent",
  "issues_url": "https://github.com/DataDog/puppet-datadog-agent/issues",
  "description": "This will install the Datadog monitoring agent for windows",
  "operatingsystem_support": [
    {
      "operatingsystem": "Windows",
      "operatingsystemrelease": ["6", "7"]
    },
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">=2016.4.x"
    },
    {
      "name": "puppet",
      "version_requirement": ">=4.6.0 <7.0.0"
    },
    {
      "name": "yumrepo_core",
      "version_requirement": ">=1.0.3 < 2.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=4.24.0 <6.0.0"
    },
    {
      "name": "puppetlabs/ruby",
      "version_requirement": ">=1.0.0 <2.0.0"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">=4.0.0 <6.0.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">=4.4.0 <=6.0.0"
    },
    {
      "name": "puppetlabs/puppetserver_gem",
      "version_requirement": ">=1.0.0 <2.0.0"
    }
    {
      "name": "puppetlabs/powershell",
      "version_requirement": ">=1.0.1"
    }
  ]
}
